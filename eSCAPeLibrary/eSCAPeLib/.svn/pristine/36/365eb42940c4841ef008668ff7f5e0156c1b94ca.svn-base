                        <value selector="604800_seconds">604800</value>
                        <value selector="1209600_seconds">1209600</value>
                        <value selector="do_not_overwrite">16777215</value>
                  </Value>
                  <Value id="retention_security_log_var" type="number" operator="greater than or equal">
                        <title>Retention Method for Security Log</title>
