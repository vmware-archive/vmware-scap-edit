        <xccdf:description>This section identifies requirements for the state of certain services on the system.</xccdf:description>
        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
        <!--                Values                              -->
        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
        <xccdf:Value id="xccdf_gov.nist_value_fax_service_var" operator="equals" type="number">
