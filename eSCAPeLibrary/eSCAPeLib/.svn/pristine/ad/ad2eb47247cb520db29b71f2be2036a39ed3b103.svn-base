        <xccdf:Rule id="xccdf_gov.nist_rule_system_integrity" selected="false" weight="10.0">
          <xccdf:title>System Integrity</xccdf:title>
          <xccdf:description>on violations of integrity of the security subsystem.</xccdf:description>
          <xccdf:ident system="http://cce.mitre.org">CCE-9520-8</xccdf:ident>
          <xccdf:ident system="http://cce.mitre.org">CCE-9194-2</xccdf:ident>
