        <xccdf:Value id="xccdf_gov.nist_value_run_administrators_admin_approval_mode_var" operator="equals" type="number">
          <xccdf:title>Run all administrators in Admin Approval Mode</xccdf:title>
          <xccdf:description>This security setting determines the behavior of all UAC policies for the entire system.</xccdf:description>
