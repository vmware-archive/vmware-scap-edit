        <!--                Rules                               -->
        <!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
        <xccdf:Rule id="xccdf_gov.nist_rule_turn_on_mapper_io_lltdio_driver" selected="false" weight="10.0">
          <xccdf:title>Turn on Mapper I/O (LLTDIO) driver</xccdf:title>
          <xccdf:description>This policy setting turns on the Mapper I/O network protocol driver. LLTDIO allows a computer to discover the topology of a network it's connected to.</xccdf:description>
          <xccdf:reference>
            <dc:type>GPO</dc:type>
            <dc:source>Computer Configuration\Administrative Templates\Network\Link-Layer Topology Discovery</dc:source>
          </xccdf:reference>
          <xccdf:ident system="http://cce.mitre.org">CCE-9783-2</xccdf:ident>
          <xccdf:check system="http://oval.mitre.org/XMLSchema/oval-definitions-5">
            <xccdf:check-export export-name="oval:gov.nist.usgcb.windowsseven:var:133" value-id="xccdf_gov.nist_value_turn_on_mapper_io_lltdio_driver_var"/>
            <xccdf:check-content-ref href="USGCB-Windows-7-oval.xml" name="oval:gov.nist.usgcb.windowsseven:def:207"/>
          </xccdf:check>
        </xccdf:Rule>
        <xccdf:Rule id="xccdf_gov.nist_rule_turn_on_responder_rspndr_driver" selected="false" weight="10.0">
          <xccdf:title>Turn on Responder (RSPNDR) driver</xccdf:title>
          <xccdf:description>This policy setting turns on the Responder network protocol driver. The Responder allows a computer to participate in Link Layer Topology Discovery requests so that it can be discovered and located on the network.</xccdf:description>
