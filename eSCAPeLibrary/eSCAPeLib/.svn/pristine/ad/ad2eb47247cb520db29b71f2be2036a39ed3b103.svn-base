          <xccdf:check system="http://oval.mitre.org/XMLSchema/oval-definitions-5">
            <xccdf:check-export export-name="oval:gov.nist.usgcb.windowsseven:var:62" value-id="xccdf_gov.nist_value_name_release_requests_var"/>
            <xccdf:check-content-ref href="USGCB-Windows-7-oval.xml" name="oval:gov.nist.usgcb.windowsseven:def:132"/>
          </xccdf:check>
        </xccdf:Rule>
