            <xccdf:check-content-ref href="USGCB-Windows-7-oval.xml" name="oval:gov.nist.usgcb.windowsseven:def:200"/>
          </xccdf:check>
        </xccdf:Rule>
        <xccdf:Rule id="xccdf_gov.nist_rule_security_state_changes" selected="false" weight="10.0">
          <xccdf:title>Security State Change</xccdf:title>
          <xccdf:description>This audit policy reports changes in security state of the system, such as when the security subsystem starts and stops.</xccdf:description>
