        <xccdf:Value id="xccdf_gov.nist_value_homegroup_listener_service_var" operator="equals" type="number">
          <xccdf:title>HomeGroup Listener Service</xccdf:title>
          <xccdf:description>Defines the startup state of the service</xccdf:description>
          <xccdf:value>4</xccdf:value>
          <xccdf:value selector="automatic">2</xccdf:value>
          <xccdf:value selector="manual">3</xccdf:value>
          <xccdf:value selector="disabled">4</xccdf:value>
        </xccdf:Value>
        <xccdf:Value id="xccdf_gov.nist_value_homegroup_provider_service_var" operator="equals" type="number">
          <xccdf:title>HomeGroup Provider Service</xccdf:title>
          <xccdf:description>Defines the startup state of the service</xccdf:description>
